{
  "expo": {
    "name": "Couteau-App",
    "slug": "Couteau-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/foto2x2.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/cajaHerramientas.png",
      "resizeMode": "contain",
      "backgroundColor": "white"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/foto2x2.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.rusbert8.Couteau-App"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7fd0bb7d-0011-47fe-a05d-59966bb768cc"
      }
    }
  }
}
